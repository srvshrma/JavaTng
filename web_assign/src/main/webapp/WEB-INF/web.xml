<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Archetype Created Web Application</display-name>
 <servlet>
  <servlet-name>CustomerController</servlet-name>
  <display-name>CustomerController</display-name>
  <servlet-class>comm.controller.CustomerController</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>CustomerView</servlet-name>
  <display-name>CustomerView</display-name>
  <servlet-class>comm.view.CustomerView</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>HomeController</servlet-name>
  <servlet-class>comm.controller.HomeController</servlet-class>
 </servlet>
 <servlet>
 	<servlet-name>UpdateController</servlet-name>
 	<display-name>UpdateController</display-name>
 	<description></description>
 	<servlet-class>comm.controller.UpdateController</servlet-class>
 </servlet>
 <servlet>
 	<servlet-name>CustomerUpdateController</servlet-name>
 	<display-name>CustomerUpdateController</display-name>
 	<description></description>
 	<servlet-class>comm.controller.CustomerUpdateController</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>CustomerController</servlet-name>
  <url-pattern>/controller.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CustomerView</servlet-name>
  <url-pattern>/customer.view</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>HomeController</servlet-name>
  <url-pattern>/homecontroller.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
 	<servlet-name>UpdateController</servlet-name>
 	<url-pattern>/updatecontroller.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
 	<servlet-name>CustomerUpdateController</servlet-name>
 	<url-pattern>/customerupdatecontroller.do</url-pattern>
 </servlet-mapping>
</web-app>
